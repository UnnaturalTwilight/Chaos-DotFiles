# Save in ~/Music
-o ~/Music/new/%(playlist|.)s/%(title)s.%(ext)s 
--windows-filenames
--sponsorblock-remove music_offtopic
--embed-metadata
# --embed-thumbnail
-f 'ba[acodec^=mp3]/ba/b' 
-x --audio-format mp3
# best quality
--audio-quality 0 
--embed-square-thumbnail

# fix yt
#--extractor-args "youtube:player_js_version=actual"